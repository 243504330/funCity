.pic{
  width:100rpx;height:100rpx;position:absolute;right:0;top:0;bottom:0;margin:auto;border-radius: 100rpx;
}
.title{
  font-size: 48rpx;
  color:#333333;
}
.inp{
  border-bottom:1px solid #e5e5e5;
  color:#333333;
  font-size: 36rpx;
  padding-bottom:20rpx;
}
.def-btn{
  margin-top:69px;
  text-align:center;
  background:#e5e5e5;
  border-radius:100rpx;
  padding-top:20rpx;
  padding-bottom:20rpx;
  color:#ffffff;
  font-size:18px;

}
.act-btn{
  margin-top:69px;
  text-align:center;
  background:linear-gradient(135deg, #D358FF 0%, #4A3AF1 100%);
  border-radius:100rpx;
  padding-top:20rpx;
  padding-bottom:20rpx;
  color:#ffffff;
  font-size:18px;
  box-shadow:inset 4px 4px 8px 0 #CF9DFF;
}
.olai{
  position:absolute;
  width:100rpx;
  height:100%;
  right:0;
  top:0;
  z-index: 999;
}
.cha{
  position:absolute;
  width:30rpx;
  height:30rpx;
  top:14rpx;
  right:20rpx;
  margin:auto;
}
.st{
  margin-top: 50rpx;
  position: relative;
}
.btn{
  text-align:center;
  margin-top:100rpx;
  background:#9fe363;
  color:#ffffff;
  line-height:94rpx;
  border-radius:8rpx;
  box-shadow:0 5px 10px 0 rgba(159,227,99,0.30);
}